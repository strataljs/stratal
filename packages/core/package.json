{
  "name": "stratal",
  "version": "0.0.1",
  "description": "A modular Cloudflare Workers framework with dependency injection, queue-based events, and type-safe configuration",
  "type": "module",
  "license": "MIT",
  "author": "Temitayo Fadojutimi",
  "homepage": "https://stratal.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strataljs/stratal.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/strataljs/stratal/issues"
  },
  "keywords": [
    "cloudflare-workers",
    "framework",
    "dependency-injection",
    "modules",
    "hono",
    "queue",
    "cron",
    "i18n",
    "openapi",
    "typescript"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js"
    },
    "./i18n": {
      "types": "./dist/i18n/index.d.ts",
      "import": "./dist/i18n/index.js"
    },
    "./i18n/messages/en": {
      "types": "./dist/i18n/messages/en/index.d.ts",
      "import": "./dist/i18n/messages/en/index.js"
    },
    "./validation": {
      "types": "./dist/i18n/validation/index.d.ts",
      "import": "./dist/i18n/validation/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js"
    },
    "./polyfills": {
      "types": "./dist/polyfills/index.d.ts",
      "import": "./dist/polyfills/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "typecheck": "yarn tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix ."
  },
  "dependencies": {
    "@hono/zod-openapi": "^1.2.2",
    "@intlify/core-base": "^11.2.8",
    "@intlify/message-compiler": "^11.2.8",
    "@scalar/hono-api-reference": "^0.9.46",
    "@xmldom/xmldom": "^0.8.11",
    "hono": "^4.12.3",
    "tsyringe": "^4.10.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/lib-storage": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@react-email/components": "^1.0.0",
    "@tus/server": "^2.0.0",
    "nodemailer": ">=7.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@aws-sdk/lib-storage": {
      "optional": true
    },
    "@aws-sdk/s3-request-presigner": {
      "optional": true
    },
    "@react-email/components": {
      "optional": true
    },
    "@tus/server": {
      "optional": true
    },
    "nodemailer": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "resend": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.1000.0",
    "@aws-sdk/lib-storage": "^3.1000.0",
    "@aws-sdk/s3-request-presigner": "3.1000.0",
    "@cloudflare/workers-types": "4.20260228.1",
    "@react-email/components": "^1.0.8",
    "@stratal/testing": "workspace:*",
    "@tus/server": "^2.3.0",
    "@types/node": "^25.3.2",
    "@types/nodemailer": "^7.0.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "nodemailer": "^8.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.9.3",
    "typescript": "^5.9.3",
    "vitest": "~3.2.4"
  }
}
